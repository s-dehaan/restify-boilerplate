{
  "name": "restify-boilerplate",
  "version": "1.0.0",
  "description": "Restify boilerplate application to get up and running quickly.",
  "main": "node app.js",
  "scripts": {
    "coverage": "nyc --reporter=lcov --reporter=text mocha --exit tests/index.js",
    "devserver": "nodemon app.js | ./node_modules/.bin/bunyan -o short",
    "start": "node app.js",
    "test": "mocha --exit",
    "test-all": "mocha --exit tests/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s-dehaan/restify-boilerplate.git"
  },
  "private": true,
  "keywords": [
    "restify",
    "boilerplate"
  ],
  "author": "Sebastiaan de Haan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/s-dehaan/restify-boilerplate/issues"
  },
  "homepage": "https://github.com/s-dehaan/restify-boilerplate#readme",
  "dependencies": {
    "bunyan": "^1.8.15",
    "restify": "^8.5.1",
    "restify-errors": "^8.0.2"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "supertest": "^6.1.3"
  }
}
